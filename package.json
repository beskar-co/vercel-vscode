{
  "name": "vercel-vscode",
  "publisher": "haydenbleasel",
  "private": true,
  "displayName": "Vercel for VS Code",
  "description": "A VS Code extension for Vercel deployment status.",
  "icon": "icon.png",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Testing",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "license": "ISC",
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/haydenbleasel/vercel-vscode"
  },
  "contributes": {
    "commands": [],
    "views": {
      "explorer": []
    },
    "configuration": {
      "title": "Vercel",
      "properties": {
        "vercel-vscode.access_token": {
          "type": "string",
          "default": "",
          "description": "Your Vercel access token"
        }
      }
    },
    "menus": {
      "view/title": [],
      "view/item/context": [],
      "commandPalette": []
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "preesbuild-base": "rm -fr out",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "dev": "tsc -watch -p ./"
  },
  "dependencies": {
    "@haydenbleasel/harmony": "^1.0.31",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.29.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.9",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "stylelint": "^14.13.0",
    "stylelint-prettier": "^2.0.0"
  },
  "devDependencies": {
    "@auto-it/first-time-contributor": "^10.37.6",
    "@auto-it/npm": "^10.37.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "auto": "^10.37.6",
    "esbuild": "^0.15.11",
    "eslint": "^8.25.0",
    "typescript": "^4.8.4"
  },
  "eslintConfig": {
    "extends": "./node_modules/@haydenbleasel/harmony/eslint.js",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "no-console": "off",
      "import/no-nodejs-modules": "off",
      "jest/require-hook": "off"
    }
  },
  "prettier": "@haydenbleasel/harmony/prettier",
  "author": "Hayden Bleasel <hello@haydenbleasel.com>"
}
