{
  "name": "vercel-vscode",
  "publisher": "haydenbleasel",
  "private": true,
  "displayName": "Vercel for VS Code",
  "description": "A VS Code extension for Vercel deployment status.",
  "icon": "icon.png",
  "version": "1.0.12",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Testing",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "license": "ISC",
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/beskar-co/vercel-vscode"
  },
  "contributes": {
    "commands": [],
    "views": {
      "explorer": []
    },
    "configuration": {
      "title": "Vercel",
      "properties": {
        "vercel-vscode.access_token": {
          "type": "string",
          "default": "",
          "description": "Your Vercel access token"
        },
        "vercel-vscode.project": {
          "type": "string",
          "default": "",
          "description": "Your Vercel project ID"
        }
      }
    },
    "menus": {
      "view/title": [],
      "view/item/context": [],
      "commandPalette": []
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "preesbuild-base": "rm -fr out",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "dev": "tsc -watch -p ./"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.29.3"
  },
  "devDependencies": {
    "@auto-it/first-time-contributor": "^10.43.0",
    "@auto-it/npm": "^10.43.0",
    "@beskar-labs/harmony": "^2.1.1",
    "@types/node": "18.x",
    "@types/vscode": "1.72.0",
    "auto": "^10.43.0",
    "esbuild": "^0.17.14",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "stylelint": "^15.4.0",
    "typescript": "^5.0.3"
  },
  "prettier": "@beskar-labs/harmony/prettier",
  "author": "Hayden Bleasel <hello@haydenbleasel.com>"
}
