{
  "name": "vercel-vscode",
  "publisher": "haydenbleasel",
  "private": true,
  "displayName": "Vercel for VS Code",
  "description": "A VS Code extension for Vercel deployment status.",
  "icon": "icon.png",
  "version": "1.0.18",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Testing",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "license": "ISC",
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/beskar-co/vercel-vscode"
  },
  "contributes": {
    "configuration": {
      "title": "Vercel",
      "properties": {
        "vercelVSCode.accessToken": {
          "type": "string",
          "default": "",
          "description": "Your Vercel Access Token"
        },
        "vercelVSCode.projectId": {
          "type": "string",
          "default": "",
          "description": "Your Vercel Project ID"
        },
        "vercelVSCode.teamId": {
          "type": "string",
          "default": "",
          "description": "Your Vercel Team ID"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsup",
    "watch": "tsup"
  },
  "dependencies": {
    "cross-fetch": "^3.1.6",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@auto-it/first-time-contributor": "^10.46.0",
    "@auto-it/npm": "^10.46.0",
    "@beskar-labs/harmony": "^2.2.8",
    "@types/node": "20.x",
    "@types/vscode": "1.72.0",
    "auto": "^10.46.0",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "prettier": "@beskar-labs/harmony/prettier",
  "author": "Hayden Bleasel <hello@haydenbleasel.com>",
  "contributors": [
    "Anna Zhang <annazhang343@gmail.com>"
  ]
}
